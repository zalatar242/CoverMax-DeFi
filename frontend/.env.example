# Build Configuration (Suppress warnings for cleaner output)
GENERATE_SOURCEMAP=false                # Disable source maps to suppress node_modules warnings
ESLINT_NO_DEV_ERRORS=true               # Treat ESLint errors as warnings in development
DISABLE_ESLINT_PLUGIN=true              # Disable ESLint plugin to reduce warnings
NODE_OPTIONS=--no-deprecation           # Suppress Node.js deprecation warnings
SKIP_PREFLIGHT_CHECK=true               # Skip Create React App preflight checks

# Network Configuration
REACT_APP_DEFAULT_NETWORK=passetHub     # The network to use (only passetHub is supported)

# Reown Configuration (Required)
REACT_APP_REOWN_PROJECT_ID=             # Required: Get it from https://cloud.reown.com

# PassETHub RPC URL (Optional - defaults to public endpoint if not provided)
REACT_APP_PASSETHUB_RPC_URL=https://testnet-passet-hub-eth-rpc.polkadot.io/
